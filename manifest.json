{
	"name": "네이버 뉴스 댓글란 숨김 확장프로그램",
	"manifest_version": 2,
	"version": "1.0",
	"content_scripts": [
		{
			"matches": [
				"*://news.naver.com/*"
			],
			"css": [
				"hide.css"
			],
			"run_at": "document_end"
		}
	],
	"permissions": [
		"*://www.news.naver.com/*"
	],
	"browser_action": {
		"default_icon": {
			"16": "icons/16.png",
			"32": "icons/32.png",
			"128": "icons/128.png"
		  }
	}
}