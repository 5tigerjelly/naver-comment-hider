{
	"name": "Naver News Comment Hider",
	"manifest_version": 2,
	"version": "0.1",
	"content_scripts": [
		{
			"matches": [
				"*://news.naver.com/*"
			],
			"css": [
				"hide.css"
			],
			"run_at": "document_end"
		}
	],
	"permissions": [
		"*://www.news.naver.com/*"
	],
	"browser_action": {
		"default_icon": {
			"16": "images/get_started16.png",
			"32": "images/get_started32.png",
			"48": "images/get_started48.png",
			"128": "images/get_started128.png"
		  }
	}
}